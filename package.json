{
  "name": "html-to-react",
  "displayName": "html-to-react",
  "description": "Converts HTML code to React JSX",
  "publisher": "VictorBaudot",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.46.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VictorBaudot/vscode-html-to-react.git"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon/html-to-react.png",
  "keywords": [
    "html",
    "conversor",
    "jsx",
    "react",
    "vue",
    "converter",
    "tailwind",
    "tailwindcss",
    "tailwindui"
  ],
  "activationEvents": [
    "onCommand:html-to-react.convert"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "html-to-react.convert",
        "title": "Convert HTML to React JSX"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
